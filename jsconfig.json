{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": [
                "src/assets/*"
            ],
            "@constants/*": [
                "src/constants/*"
            ],
            "@containers/*": [
                "src/containers/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@/*": [
                "src/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@InfoParagraph/*": [
                "src/components/InfoParagraph/*"
            ],
            "@CustomInfoParagraph/*": [
                "src/components/CustomInfoParagraph/*"
            ],
            "@CustomContentHeader/*": [
                "src/components/CustomContentHeader/*"
            ]
        }
    }
}